
<section class="site-section bg-light block-13" id="testimonials-section" data-aos="fade">
    <div class="container">
        <div class="btn-group">
            <a href="/" data-tooltip="tooltip" data-placement="top" title="REGRESAR" class="btn btn-outline-primary btn-sm"><i class="fas fa-arrow-left fa-2x"></i></a>
        </div>
        <div class="card">
            <div class="card-header bg-succses">
                HISTORIAL CITAS MEDICAS
            </div>
            <div class="card-body">
                <div class="form-group">

                    <div style="font-size: 20px; color: #b21f2d;">
                        {{msg.error}}
                    </div>
                    <div style="font-size: 20px; color: green;">
                        {{msg.ok}}
                    </div>
                    <div style="font-size: 20px; color: green;">
                        {{msg.info}}
                    </div>
                </div>
                <div class="form-group">
                    <div class="table-responsive">
                        <table id="tablaLCliente" class="table table-sm">
                            <thead>
                                <tr>
                                    <th>Nro</th>
                                    <th>Cliente</th>
                                    <th>Mascota</th>
                                    <th>Servicio</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                {{#each cita as |lis i|}}
                                <tr>
                                    <td>
                                        {{i}}
                                    </td>
                                    <td>
                                        {{id_cliente}}
                                    </td>
                                    <td>
                                        {{id_mascota}}
                                    </td>
                                    <td>
                                        {{id_servicio}}
                                    </td>
                                  
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>